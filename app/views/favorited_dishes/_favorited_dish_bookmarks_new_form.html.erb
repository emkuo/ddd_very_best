<%= form_with(model: @bookmark, local: true) do |f| %>

  <div class="form-group">
    <%= f.label :user_id %>

    <%= f.collection_select :user_id, User.all, :id, :to_s, {}, class: "form-control" %>
  </div>

  <%= f.hidden_field :dish_id, value: @favorited_dish.id %>

  <div class="form-group">
    <%= f.label :venues_id %>

    <%= f.collection_select :venues_id, Venue.all, :id, :to_s, {}, class: "form-control" %>
  </div>

  <%= f.submit "Create bookmark", class: "btn btn-block btn-outline-secondary" %>
<% end %>
