<!-- Form bookmark start -->
<%= form_with(model: @bookmark, local: true) do |f| %>
<!-- Form bookmark end -->

  <!-- Label and input for user_id start -->
  <!-- Input for user_id start -->
  <%= f.hidden_field :user_id, value: @user.id %>
  <!-- Input for user_id end -->
  <!-- Label and input for user_id end -->

  <!-- Label and input for dish_id start -->
  <div class="form-group">
    <%= f.label :dish_id %>

    <!-- Input for dish_id start -->
    <%= f.collection_select :dish_id, FavoritedDish.all, :id, :to_s, {}, class: "form-control" %>
    <!-- Input for dish_id end -->
  </div>
  <!-- Label and input for dish_id end -->

  <!-- Label and input for venues_id start -->
  <div class="form-group">
    <%= f.label :venues_id %>

    <!-- Input for venues_id start -->
    <%= f.text_field :venues_id, class: "form-control" %>
    <!-- Input for venues_id end -->
  </div>
  <!-- Label and input for venues_id end -->

  <%= f.submit "Create bookmark", class: "btn btn-block btn-outline-secondary" %>
<% end %>
